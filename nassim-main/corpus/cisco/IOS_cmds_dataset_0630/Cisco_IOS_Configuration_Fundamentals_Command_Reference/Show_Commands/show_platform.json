{
    "From": " Cisco IOS Configuration Fundamentals Command Reference",
    "PageTitle": "show platform",
    "FuncDef": [
        "To display platform information, use the **show platform**command in privileged EXEC mode. "
    ],
    "CLIs": [
        "show platform { buffers | copp rate-limit { arp | dhcp | atm-oam | ethernet-oam | icmp | igmp | pppoe-discovery | atom ether-vc | all } | np copp [ <ifnum> ] [ detail ] | dma | eeprom | fault | hardware capacity | hardware pfc mode | internal-vlan | interrupts | netint | software ipv6-multicast connected | stats | tech-support { ipmulticast [ vrf <vrf-name> ] <group-ip-addr> <src-ip-addr> | unicast [ vrf <vrf-name> ] <destination-ip-addr> <destination-mask> [ global ] } | tlb | vfi dot1q-transparency | vlans }",
        "Cisco ASR 1000 Series Aggregation Services Routers",
        "show platform"
    ],
    "ParentView": [
        null
    ],
    "ParaDef": [
        {
            "Parameters": " **buffers** ",
            "Info": " Displays buffer-allocation information. "
        },
        {
            "Parameters": " **icmp** ",
            "Info": " Specifies Internet Connection Management Protoocol Rate limiter. "
        },
        {
            "Parameters": " **igmp** ",
            "Info": " Specifies Internet Group Management Potocol Rate limiter. "
        },
        {
            "Parameters": " **atom ether-vc** ",
            "Info": " Shows whether IP or routed mode interworking is configured. "
        },
        {
            "Parameters": " *ifnum* ",
            "Info": " (Optional) A session ID. "
        },
        {
            "Parameters": " **dma** ",
            "Info": " Displays Direct Memory Access (DMA) channel information. "
        },
        {
            "Parameters": " **eeprom** ",
            "Info": " Displays CPU EEPROM information. "
        },
        {
            "Parameters": " **fault** ",
            "Info": " Displays the fault date. "
        },
        {
            "Parameters": " **hardware pfc mode** ",
            "Info": " Displays the type of installed Policy Feature Card (PFC). "
        },
        {
            "Parameters": " **internal-vlan** ",
            "Info": " Displays the internal VLAN. "
        },
        {
            "Parameters": " **interrupts** ",
            "Info": " Displays m8500 interrupt counters. "
        },
        {
            "Parameters": " **netint** ",
            "Info": " Displays the platform network-interrupt information. "
        },
        {
            "Parameters": " **stats** ",
            "Info": " Displays Constellation WAN (CWAN) statistics. "
        },
        {
            "Parameters": " **tech-support ipmulticast** ",
            "Info": " Displays IP multicast-related information for Technical Assistance Center (TAC). "
        },
        {
            "Parameters": " **vrf** *vrf-name* ",
            "Info": " (Optional) Displays the Virtual Private Network (VPN) routing and forwarding (VRF) instance. "
        },
        {
            "Parameters": " *group-ip-addr* ",
            "Info": " Group IP address. "
        },
        {
            "Parameters": " *src-ip-addr* ",
            "Info": " Source IP address. "
        },
        {
            "Parameters": " **unicast** ",
            "Info": " Displays IP unicast-related information for TAC. "
        },
        {
            "Parameters": " *destination-ip-addr* ",
            "Info": " Destination IP address. "
        },
        {
            "Parameters": " *destination-mask* ",
            "Info": " Destination mask. "
        },
        {
            "Parameters": " **global** ",
            "Info": " (Optional) Displays global output. "
        },
        {
            "Parameters": " **tlb** ",
            "Info": " Displays information about the translation look-aside buffer (TLB) register. "
        },
        {
            "Parameters": " **vfi** ",
            "Info": " Displays CWAN virtual forwarding instance (VFI) commands. "
        },
        {
            "Parameters": " **dot1q-transparency** ",
            "Info": " Displays the dot1q transparency setting. "
        },
        {
            "Parameters": " **vlans** ",
            "Info": " Displays hidden VLAN-to-WAN interface mapping. "
        }
    ],
