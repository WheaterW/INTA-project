{
    "From": " Cisco IOS Configuration Fundamentals Command Reference",
    "PageTitle": "show pagp",
    "FuncDef": [
        "To display port-channel information, use the **show pagp** command in user EXEC or privileged EXEC mode. "
    ],
    "CLIs": [
        "show pagp [ <group-number> ] { counters | internal | neighbor | pgroup }"
    ],
    "ParentView": [
        null
    ],
    "ParaDef": [
        {
            "Parameters": " *group-number* ",
            "Info": " (Optional) Channel-group number; valid values are a maximum of 64 values from 1 to 282. "
        },
        {
            "Parameters": " **counters** ",
            "Info": " Displays the traffic information. "
        },
        {
            "Parameters": " **internal** ",
            "Info": " Displays the internal information. "
        },
        {
            "Parameters": " **neighbor** ",
            "Info": " Displays the neighbor information. "
        },
        {
            "Parameters": " **pgroup** ",
            "Info": " Displays the active port channels. "
        }
    ],
    "Examples": [
