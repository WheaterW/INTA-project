{
    "From": " Cisco IOS Configuration Fundamentals Command Reference",
    "PageTitle": "show usb device",
    "FuncDef": [
        "To display USB device information, use the **show usb device** command in privileged EXEC mode. "
    ],
    "CLIs": [
        "show usb device [ <controller-ID> [ <device-address> ] ]"
    ],
    "ParentView": [
        null
    ],
    "ParaDef": [
        {
            "Parameters": " *controller-ID* ",
            "Info": " (Optional) Displays information only for the devices under the specified controller. "
        },
        {
            "Parameters": " *device-address* ",
            "Info": " (Optional) Displays information only for the device with the specified address. "
        }
    ],
