{
    "From": " Cisco IOS Configuration Fundamentals Command Reference",
    "PageTitle": "show html",
    "FuncDef": [
        "To display module and port information, use the **show html** command in privileged EXEC mode. "
    ],
    "CLIs": [
        "show html { module [ ports [ l2 ] ] | port [ all | l2 | l3 ] [ shortnames ] } { command <line> | count | names | options }"
    ],
    "ParentView": [
        null
    ],
    "ParaDef": [
        {
            "Parameters": " **module** ",
            "Info": " Displays module information. "
        },
        {
            "Parameters": " **ports** ",
            "Info": " (Optional) Displays the number of ports on the module. "
        },
        {
            "Parameters": " **l2** ",
            "Info": " (Optional) Displays information about the Layer2 (l2) module. "
        },
        {
            "Parameters": " **port** ",
            "Info": " Displays port information. "
        },
        {
            "Parameters": " **all** ",
            "Info": " (Optional)Displays information about the Layer 2 and Layer 3 modules. "
        },
        {
            "Parameters": " **l2** ",
            "Info": " (Optional) Displays information about the Layer2 (l2) module. "
        },
        {
            "Parameters": " **l3** ",
            "Info": " (Optional) Displays information about the Layer3 (l3) module. "
        },
        {
            "Parameters": " **shortnames** ",
            "Info": " (Optional) Displays port short names. "
        },
        {
            "Parameters": " **command** ",
            "Info": " Displays execute command over ports information. "
        },
        {
            "Parameters": " *line* ",
            "Info": " Displays command to execute over modules information. "
        },
        {
            "Parameters": " **count** ",
            "Info": " Displays the module count. "
        },
        {
            "Parameters": " **names** ",
            "Info": " Displays the module names. "
        },
        {
            "Parameters": " **options** ",
            "Info": " Displays the module options. "
        }
    ],
    "Examples": [
