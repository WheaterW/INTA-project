{
    "From": " Cisco IOS Configuration Fundamentals Command Reference",
    "PageTitle": "show module",
    "FuncDef": [
        "To display the module status and information, use the **show** module command in user EXEC or privileged EXEC mode. "
    ],
    "CLIs": [
        "show module [ mod-num | all | provision | version ]"
    ],
    "ParentView": [
        null
    ],
    "ParaDef": [
        {
            "Parameters": " *mod*-*num* ",
            "Info": " (Optional) Number of the module. "
        },
        {
            "Parameters": " **all** ",
            "Info": " (Optional) Displays the information for all modules. "
        },
        {
            "Parameters": " **provision** ",
            "Info": " (Optional) Displays the status about the module provisioning. "
        }
