{
    "From": " Cisco IOS Configuration Fundamentals Command Reference",
    "PageTitle": "show platform subscriber-group",
    "FuncDef": [
        "To display the subscriber group information, use the **show platform subscriber-group** command in privileged EXEC mode. "
    ],
    "CLIs": [
        "show platform subscriber-group { <vrf-number> | all } [ detail ]"
    ],
    "ParentView": [
        null
    ],
    "ParaDef": [
        {
            "Parameters": " *vrf-number* ",
            "Info": " VRF identification number. Displays VPN routing and forwarding (VRF) information for the specified VRF ID. "
        },
        {
            "Parameters": " **all** ",
            "Info": " Displays information about all VRFs. "
        },
        {
            "Parameters": " **detail** ",
            "Info": " Displays detailed information about the subscriber group. "
        }
    ],
    "Examples": [
        [
            "```",
            "Router#show platform subscriber-group all",
            "```",
            "```",
            "Container0 [ :0 ] No of access sub-if(s) 1",
            "```",
            "```",
            "Vlan 1014 p_cnt 1 Old Vlan 0 ip T",
            "```",
            "```",
            "Container2 [ VRF2:2 ] No of access sub-if(s) 1",
            "```",
            "```",
            "Vlan 1018 p_cnt 1 Old Vlan 0 ip T",
            "```",
            "```",
            "```"
        ],
        [
            "```",
            "Router#show platform subscriber-group 0 detail",
            "```",
            "```",
            "------------------------------------------",
            "```",
            "```",
            "VRF [ :0 ] Container0 No of access sub-if(s) 1 Vlan 1014",
            "```",
            "```",
            "Access Interfaces:",
            "```",
            "```",
            "GigabitEthernet2/10.2",
            "```"
        ]
    ],
    "ExtraInfo": null,
    "RelatedCmds": [
        {
            "cmd": " **show platform** ",
            "description": " Displays platform information. "
        }
    ]
}