    "PageTitle": "request platform software package expand file",
    "FuncDef": [
        "To extract the individual modules from a Cisco IOS-XE image, use the **request platform software package expand file** command in privileged EXEC or diagnostic mode. "
    ],
    "CLIs": [
        "request platform software package expand file <source-URL> [ to <destination-URL> ] [ force ] [ verbose ] [ wipe ]"
    ],
    "ParentView": [
        null
    ],
    "ParaDef": [
        {
            "Parameters": " **verbose** ",
            "Info": " (Optional) Displays verbose information, meaning all output that can be displayed on the console during the process will be displayed. "
        },
        {
            "Parameters": " **wipe** ",
            "Info": " (Optional) Erases all content on the destination snapshot directory before extracting the files and placing them on the snapshot directory. "
        }
    ],
