{
    "From": " Cisco IOS Configuration Fundamentals Command Reference",
    "PageTitle": "show registry",
    "FuncDef": [
        "To display the function registry information when Cisco IOS or Cisco IOS Software Modularity images are running, use the show registry command in user EXEC or privileged EXEC mode. "
    ],
    "CLIs": [
        "show registry [ <registry-name> [ <registry-number> ] ] [ brief | statistics ]",
        "<><><><><><><><><>Cisco<><><><><><><><><> <><><><><><><><><>IOS<><><><><><><><><> <><><><><><><><><>Software<><><><><><><><><> <><><><><><><><><>Modularity<><><><><><><><><>",
        "show registry [ name [ <registry-name> [ <registry-number> ] ] ] [ brief [ name [ <registry-name> [ <registry-number> ] ] ] | preemptions | rpcp status | statistics [ brief ] [ name [ <registry-name> [ <registry-number> ] ] ] [ remote ] ] [ process { <process-name> | <process-id> } ]"
    ],
    "ParentView": [
        null
    ],
    "ParaDef": [
        {
            "Parameters": " *registry-name* ",
            "Info": " (Optional) Name of the registry to display. "
        },
        {
            "Parameters": " *registry-number* ",
            "Info": " (Optional) Number of the registry to display. "
        },
        {
            "Parameters": " **brief** ",
            "Info": " (Optional) Displays limited functions and services information. "
        },
        {
            "Parameters": " **statistics** ",
            "Info": " (Optional) Displays function registry statistics. "
        },
        {
            "Parameters": " **name** ",
            "Info": " (Optional) Displays information about a specific registry. "
        },
        {
            "Parameters": " *registry-name* ",
            "Info": " (Optional) Name of the registry to examine. "
        },
        {
            "Parameters": " *registry-number* ",
            "Info": " (Optional) Number of the registry to examine. "
        },
        {
            "Parameters": " **brief** ",
            "Info": " (Optional) Displays limited functions and services information. "
        },
        {
            "Parameters": " **preemptions** ",
            "Info": " (Optional) Displays registry preemptions information. "
        },
        {
            "Parameters": " **rpcp status** ",
            "Info": " (Optional) Displays status of remote procedure call (RPC) proxy. "
        },
        {
            "Parameters": " **statistics** ",
            "Info": " (Optional) Displays function registry statistics. "
        },
        {
            "Parameters": " **remote** ",
            "Info": " (Optional) Displays name server interactions and call statistics. "
        },
        {
            "Parameters": " **process** ",
            "Info": " (Optional) Displays process-specific information. "
        },
        {
            "Parameters": " *process-name* ",
            "Info": " (Optional) Process name. "
        },
        {
            "Parameters": " *process-id* ",
            "Info": " (Optional) Process ID. Number in range from 1 to 4294967295. "
        }
