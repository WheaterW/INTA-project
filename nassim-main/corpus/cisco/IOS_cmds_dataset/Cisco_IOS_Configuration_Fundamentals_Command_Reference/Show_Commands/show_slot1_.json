{
    "From": " Cisco IOS Configuration Fundamentals Command Reference",
    "PageTitle": "show slot1:",
    "FuncDef": [
        "To display information about the PCMCIA flash memory card's file system located in slot 1, use the **show slot1:** command in user EXEC or privileged EXEC mode. "
    ],
    "CLIs": [
        "show slot1: [ all | chips | detailed | err | summary ]"
    ],
    "ParentView": [
        null
    ],
    "ParaDef": [
        {
            "Parameters": " **all** ",
            "Info": " (Optional) Displays all possible flash system information for all PCMCIA flash cards in the system. "
        },
        {
            "Parameters": " **chips** ",
            "Info": " (Optional) Displays flash chip information. "
        },
        {
            "Parameters": " **detailed** ",
            "Info": " (Optional) Displays the flash detailed directory. "
        },
        {
            "Parameters": " **err** ",
            "Info": " (Optional) Displays the flash chip erase and write retries. "
        },
        {
            "Parameters": " **summary** ",
            "Info": " (Optional) Displays the flash partition summary. "
        }
    ],
