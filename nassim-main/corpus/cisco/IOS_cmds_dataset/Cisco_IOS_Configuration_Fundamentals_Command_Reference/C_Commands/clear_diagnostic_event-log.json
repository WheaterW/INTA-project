{
    "From": " Cisco IOS Configuration Fundamentals Command Reference",
    "PageTitle": "clear diagnostic event-log",
    "FuncDef": [
        "To clear the diagnostic event logs for a specific module or event type, use the **clear diagnostic event-log** command in Priveleged exec mode. "
    ],
    "CLIs": [
        "<><><><><>clear<><><><><> <><><><><>diagnostic<><><><><> <><><><><>event<><><><><>-<><><><><>log<><><><><> { <><><><><>event<><><><><>-<><><><><>type<><><><><> { <><><><><>error<><><><><> | <><><><><>info<><><><><> | <><><><><>warning<><><><><> } | <><><><><>module<><><><><> { <><><><><>num<><><><><> | <><><><><>slot<><><><><> <><><><><>subslot<><><><><> | <><><><><>all<><><><><> }"
    ],
    "ParentView": [
        null
    ],
    "ParaDef": [
        {
            "Parameters": " **event-type error** ",
            "Info": " Specifies clearing error events. "
        },
        {
            "Parameters": " **event-type info** ",
            "Info": " Specifies clearing informative events. "
        },
        {
            "Parameters": " **event-type warning** ",
            "Info": " Specifies clearing warning events. "
        },
        {
            "Parameters": " **module** *num* ",
            "Info": " *slot subslot* "
        },
        {
